@using ViewModels.AppSettings.MobileFiles.Dropbox;
@model DropboxDetailViewModel
@{
    Layout = null;
}

<div class="row">
    <div class="col-lg-12">
        <div class="row">
            <div class="col-lg-6">
                <div class="mb-3">
                    <label asp-for="Url" class="form-label fw-600"></label>
                    <div>@(string.IsNullOrEmpty(Model.Url) ? "-" : Model.Url)</div>
                </div>
            </div>
            <div class="col-lg-6">
                <div class="mb-3">
                    <label asp-for="ActiveStatus" class="form-label fw-600"></label>
                    <div>@Model.ActiveStatus</div>
                </div>
            </div>
        </div>
    </div>
</div>

