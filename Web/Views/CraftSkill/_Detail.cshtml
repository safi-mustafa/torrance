@using ViewModels.OverrideLogs;
@model CraftSkillDetailViewModel
@{
    Layout = null;
}

<div class="row">
    <div class="col-lg-12">
        <div class="row">
            <div class="col-lg-6">
                <div class="mb-3">
                    <label asp-for="Name" class="form-label fw-600"></label>
                    <div>@(string.IsNullOrEmpty(Model.Name) ? "-" : Model.Name)</div>
                </div>
            </div>
            <div class="col-lg-6">
                <div class="mb-3">
                    <label asp-for="STRate" class="form-label fw-600"></label>
                    <div class="input-currency">@Model.STRate</div>
                </div>
            </div>
            <div class="col-lg-6">
                <div class="mb-3">
                    <label asp-for="OTRate" class="form-label fw-600"></label>
                    <div class="input-currency">@Model.OTRate</div>
                </div>
            </div>
            <div class="col-lg-6">
                <div class="mb-3">
                    <label asp-for="DTRate" class="form-label fw-600"></label>
                    <div class="input-currency">@Model.DTRate</div>
                </div>
            </div>
        </div>
    </div>
</div>

