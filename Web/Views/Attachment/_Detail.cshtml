@using ViewModels.Shared;
@model AttachmentVM
@{
    Layout = null;
}

<div class="row">
    <div class="col-lg-12">
        <div class="row">
            <div class="col-lg-12">
                <div class="mb-3">
                    <label asp-for="Name" class="form-label fw-600"></label>
                    <div>@(string.IsNullOrEmpty(Model.Name) ? "-" : Model.Name)</div>
                </div>
            </div>
        </div>
        <div class="row" style="margin-top: 50px; height: 200px;">
            <label asp-for="Url" class="form-label fw-600">Uploaded Icon</label>
            <div class="card-img-actions d-inline-block mb-3" style="margin-left: 10px;">
                <img class="img-profile-fluid img-style" src="@Model.Url" alt="" width="150" height="110">
            </div>
        </div>
    </div>




</div>

