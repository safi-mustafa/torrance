@model WRRLogSearchViewModel
@using ViewModels.WeldingRodRecord.WRRLog;

<div class="row">
    <div class="col-md-4">
        <div class="form-group">
            <label asp-for="Email" class="control-label form-label fw-600"></label>
            <input asp-for="Email" type="search" class="form-control" placeholder="Search Email" />
        </div>
    </div>
    <div class="col-lg-4">
        <div class="mb-3">
            <partial name="~/Views/Shared/Select2/_Department.cshtml" for="Department" />
        </div>
    </div>
    <div class="col-lg-4">
        <div class="mb-3">
            <partial name="~/Views/Shared/Select2/_Employee.cshtml" for="Employee" />
        </div>
    </div>
    
</div>
<div class="row">
    <div class="col-lg-4">
        <div class="mb-3">
            <partial name="~/Views/Shared/Select2/_RodType.cshtml" for="RodType" />
        </div>
    </div>
    <div class="col-lg-4">
        <div class="mb-3">
            <partial name="~/Views/Shared/Select2/_WeldMethod.cshtml" for="WeldMethod" />
        </div>
    </div>
    <div class="col-lg-4">
        <div class="mb-3">
            <partial name="~/Views/Shared/Select2/_Location.cshtml" for="Location" />
        </div>
    </div>
</div>
<div class="row">
    <div class="col-lg-4">
        <div class="mb-3">
            <partial name="~/Views/Shared/Select2/_Unit.cshtml" for="Unit" />
        </div>
    </div>
